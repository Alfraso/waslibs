<?xml version="1.0"?>
<package >
  <metadata>
    <id>WindowsAppStudio.Controls</id>
    <version>$version$</version>
    <title>Windows AppStudio Controls (UWP)</title>
    <authors>Windows AppStudio Team</authors>
    <owners>wasteam</owners>
    <licenseUrl>https://raw.githubusercontent.com/wasteam/waslibs/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/wasteam/waslibs</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>XAML controls for Windows 10 apps created with Windows AppStudio.</description>
    <releaseNotes>Responsive Grid View, ReadingWebView, Visual BreakPoints and more..</releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>Windows AppStudio XAML UWP Responsive</tags>
    <references>
      <reference file="AppStudio.Controls.dll" />
    </references>
    <dependencies>
      <dependency id="WindowsAppStudio.Common" version="$version" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\*.dll"         target="lib\uap10.0" />
    <file src="bin\$configuration$\*.pdb"         target="lib\uap10.0" />
    <file src="bin\$configuration$\*.pri"         target="lib\uap10.0" />
    <file src="bin\$configuration$\**\*.xbf"      target="lib\uap10.0\AppStudio.Controls" />
    <file src="bin\$configuration$\*.xr.xml"      target="lib\uap10.0\AppStudio.Controls" />
    <file src="Properties\Default.rd.xml"         target="lib\uap10.0\AppStudio.Controls\Properties" />
  </files>
</package>